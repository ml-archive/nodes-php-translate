{
    "name": "nodes/translate",
    "description": "Enable usage of translation services rather than local files",
    "keywords": [
        "nodes",
        "translate",
        "translations",
        "nstack",
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Morten Rugaard",
            "email": "moru@nodes.dk",
            "role": "Developer"
        }, {
          "name": "Casper Rasmussen",
          "email": "cr@nodes.dk",
          "role": "CTO"
        }
    ],
    "require": {
        "laravel/framework": "5.1.*||5.2.*||5.3.*||5.4.*||5.5.*||5.6.*||5.7.*||5.8.*||6.*",
        "nodes/core": "1.1.*"
    },
    "autoload": {
        "psr-4": {
            "Nodes\\Translate\\": "src"
        },
        "files": [
            "src/Support/Helpers/Translate.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nodes\\Translate\\ServiceProvider"
            ],
            "aliases": {
                "Translate": "Nodes\\Translate\\Support\\Facades\\Translate"
            }
        }
    }
}
